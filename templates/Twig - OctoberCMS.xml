  <template name="ife" value="{% if $CONDITION$ %}&#10;&#9;$STUFF$&#10;{% else %}&#10;&#9;$MORESTUFF$&#10;{% endif %}" description="if...else...endif" toReformat="false" toShortenFQNames="true">
