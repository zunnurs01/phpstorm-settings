  </template>
  <template name="iph" value="{% put $STUFF$ %}&#10;    $MORESTUFF$&#10;{% endput %}" description="inject: placeholder variable" toReformat="false" toShortenFQNames="true">
    <variable name="STUFF" expression="" defaultValue="&quot;variable&quot;" alwaysStopAt="true" />
    <variable name="MORESTUFF" expression="" defaultValue="&quot;your code&quot;" alwaysStopAt="true" />
    <context>
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="phd" value="{% placeholder $STUFF$ default %}&#10;    $END$&#10;{% endplaceholder %}" description="placeholder + default" toReformat="false" toShortenFQNames="true">
    <variable name="STUFF" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="iphd" value="{% put $VAR$ %}&#10;    $STUFF$&#10;    {% default %}&#10;{% endput %}" description="inject: placeholder variable + default" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="&quot;variable&quot;" alwaysStopAt="true" />
    <variable name="STUFF" expression="" defaultValue="&quot;your code&quot;" alwaysStopAt="true" />
